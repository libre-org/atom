  },
  "bugs": {
    "url": "https://github.com/libre-org/atom/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {},
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
